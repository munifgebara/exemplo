{
  "name": "gumga-many-to-many-ng",
  "version": "1.0.2",
  "description": "Gumga Many To Many",
  "main": "./src/many-to-many.js",
  "scripts": {
    "start": "npm-run-all -p live-server watch",
    "live-server": "live-server --host=localhost --port=9000",
    "watch": "simplifyify 'src/query-action.js' -o 'dist/gumga-query-action.js' --bundle --minify --watch --debug",
    "prod": "simplifyify 'src/query-action.js' -o 'dist/gumga-query-action.js' --bundle --minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gumga",
    "many to many",
    "angular 1.x"
  ],
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "angular": "1.5.8"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "angular-ui-bootstrap": "2.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "gumga-controller-ng": "^1.0.0",
    "gumga-generic-filter-ng": "^1.0.0",
    "gumga-list-ng": "^1.0.0",
    "gumga-query-filter-ng": "^1.0.0",
    "gumga-rest-ng": "^1.0.0",
    "live-server": "^1.1.0",
    "npm-run-all": "^2.3.0",
    "simplifyify": "^3.2.1"
  },
  "author": "Gumga Tecnologia da Informação S/A",
  "license": "LGPL-3.0"
}
